#include <iostream>
using namespace std;

inline void swap(int &i, int &j)
{
    int temp=i;
    i=j;
    j=temp;
}
inline void swap(double &i, double &j)
{
    double temp = i;
    i=j;
    j=temp;
}
int main()
{
    int m=5, n=10;
    double x=5.5, y=6.7;
    
    cout << "inputs:" << m << "," << n << endl;
    swap (m, n);
    cout<< "output" << m << "," << n << endl;
    
    cout << "double inputs :" << x<<"," << y<< endl;
    swap(x,y);
    cout << "double output"<< x<<"," << y<< endl;
    
}
