#include <iostream>
using namespace std;
long long fact(int n)
{
    long long f=1;
    for(int i=1; i<=n; i++)
    f*=i;
    return f;
}
int main(void)
{
    cout <<"this program is crazy";
    for(auto i=0; i<5;i++)
    {
        cout << "factorial of" << i << "is" << fact(i) << endl;
    }
    cout << "thats all"<< endl;
    
}
